<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <RootNamespace>netextensions.WindowsService</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Title>netextensions-WindowsService</Title>
    <Description>WindowsService for .net core</Description>
    <PackageProjectUrl>https://netextensions.io/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/netextensions/netextensions-WindowsService</RepositoryUrl>
    <PackageTags>.net core, windows service</PackageTags>
    <PackageId>netextensions-WindowsService-preview</PackageId>
    <PackageVersion>0.0.0.1</PackageVersion>
    <PackageLicenseUrl>https://en.wikipedia.org/wiki/MIT_License</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LanguageExt.Core" Version="3.3.7" />
    <PackageReference Include="Microsoft.AspNetCore.Hosting.WindowsServices" Version="3.0.0-preview7.19365.7" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="3.0.0-preview7.19362.4" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.0.0-preview7.19362.4" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
  </ItemGroup>

</Project>
